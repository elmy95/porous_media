FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      mDot;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    "sym.*"
    {
        type            symmetryPlane;
    }

    outlet
    {
      type            zeroGradient;
      
    }

    faceWall
    {
        type           zeroGradient;
    }
     fluid_to_solid
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}

